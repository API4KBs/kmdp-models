              <minDepth>1</minDepth>
              <schemeName>DependencyTypes</schemeName>
              <topConceptName>DependencyType</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>rel-series-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>https://www.omg.org/spec/API4KP/20190801/api4kp/</owlFile>
              <targetNamespace>https://www.omg.org/spec/API4KP/api4kp/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/in-series-with</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/Rel/RelatedVersionType</skosNamespace>
              <skosOutputFile>rel-series.skos.rdf</skosOutputFile>
              <minDepth>1</minDepth>
              <schemeName>RelatedVersionTypes</schemeName>
              <topConceptName>RelatedVersionType</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>rel-part-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>https://www.omg.org/spec/API4KP/20190801/api4kp/</owlFile>
              <targetNamespace>https://www.omg.org/spec/API4KP/api4kp/</targetNamespace>
