              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/KnowledgeProcessingTechnique</skosNamespace>
              <skosOutputFile>kpm.skos.rdf</skosOutputFile>
              <minDepth>1</minDepth>
