              <skosOutputFile>rel-variant.skos.rdf</skosOutputFile>
              <schemeName>VariantTypes</schemeName>
              <topConceptName>VariantType</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>rel-summary-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
