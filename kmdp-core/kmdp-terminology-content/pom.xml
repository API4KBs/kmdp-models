            </configuration>
          </execution>
          <execution>
            <id>prov-pub-event</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/ontologies/KMDO/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/ontologies/KMDO/</targetNamespace>
              <targetURI>http://ontology.mayo.edu/ontologies/KMDO/KnowledgeManagementActivity</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/PublicationEventType</skosNamespace>
              <skosOutputFile>prov-pub-ev.skos.rdf</skosOutputFile>
              <schemeName>PublicationEventTypes</schemeName>
              <minDepth>1</minDepth>
              <topConceptName>PublicationEventType</topConceptName>
            </configuration>
          </execution>


          <execution>
            <id>km-about</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <entityType>OBJ_PROP</entityType>
              <owlFile>http://ontology.mayo.edu/ontologies/KMDO/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/ontologies/KMDO/</targetNamespace>
              <targetURI>http://ontology.mayo.edu/ontologies/KMDO/associated-with</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KMDO/AnnotationRelType</skosNamespace>
              <skosOutputFile>ann.skos.rdf</skosOutputFile>
              <schemeName>AnnotationRelTypes</schemeName>
              <minDepth>1</minDepth>
              <topConceptName>AnnotationRelType</topConceptName>
            </configuration>
          </execution>

          <execution>
            <id>mime</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
