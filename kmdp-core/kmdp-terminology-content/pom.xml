              <schemeName>StructuralPartTypes</schemeName>
              <topConceptName>StructuralPartType</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>rel-part-cite</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://purl.org/spar/cito</owlFile>
              <targetNamespace>http://purl.org/spar/cito</targetNamespace>
              <targetURI>http://purl.org/spar/cito/cites</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/Rel/CitationRelType</skosNamespace>
              <skosOutputFile>rel-cites.skos.rdf</skosOutputFile>
              <schemeName>BibliographicCitationTypes</schemeName>
              <topConceptName>BibliographicCitationType</topConceptName>
            </configuration>
          </execution>

          <execution>
            <id>prov-pro</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <entityType>INST</entityType>
              <owlFile>http://purl.org/spar/pro</owlFile>
              <targetNamespace>http://purl.org/spar/pro</targetNamespace>
              <targetURI>http://purl.org/spar/pro/PublishingRole</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/PublishingRole</skosNamespace>
              <skosOutputFile>prov-pro.skos.rdf</skosOutputFile>
              <schemeName>PublishingRoles</schemeName>
              <topConceptName>PublishingRole</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>prov-pso</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <entityType>INST</entityType>
              <owlFile>http://purl.org/spar/pso</owlFile>
              <targetNamespace>http://purl.org/spar/pso</targetNamespace>
