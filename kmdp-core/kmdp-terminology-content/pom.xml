<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

  <parent>
    <artifactId>kmdp-core</artifactId>
    <groupId>edu.mayo.kmdp</groupId>
    <version>2.0.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <artifactId>kmdp-terminology-content</artifactId>
  <name>KMDP :: Terms :: Terminology, Ontology and Vocabulary Content</name>

  <properties>
    <packageName>edu.mayo.kmdp.vocabs</packageName>
    <s>${file.separator}</s>
    <packageDir>edu${s}mayo${s}kmdp${s}vocabs</packageDir>
    <ontologyDir>${project.basedir}${s}src${s}main${s}resources${s}owl${s}${packageDir}
    </ontologyDir>
    <skosDir>${project.build.directory}${s}generated-sources${s}skos${s}${packageDir}</skosDir>
  </properties>

  <dependencies>
    <dependency>
      <groupId>edu.mayo.kmdp</groupId>
      <artifactId>kmdp-terminology-model</artifactId>
    </dependency>
    <dependency>
      <groupId>org.omg.spec</groupId>
      <artifactId>API4KP</artifactId>
    </dependency>

    <dependency>
      <groupId>de.escalon.hypermedia</groupId>
      <artifactId>hydra-jsonld</artifactId>
    </dependency>

    <dependency>
      <groupId>org.reflections</groupId>
      <artifactId>reflections</artifactId>
    </dependency>


    <dependency>
      <groupId>edu.mayo.kmdp</groupId>
      <artifactId>kmdp-test-utils</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>net.sourceforge.owlapi</groupId>
      <artifactId>owlapi-api</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>net.sourceforge.owlapi</groupId>
      <artifactId>owlapi-apibinding</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>edu.stanford.protege</groupId>
      <artifactId>org.protege.xmlcatalog</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.fasterxml.jackson.dataformat</groupId>
      <artifactId>jackson-dataformat-xml</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>

    <resources>
      <resource>
        <directory>target/generated-sources</directory>
        <includes>
          <include>**/*.xjb</include>
          <include>**/*.xsd</include>
          <include>**/*.rdf</include>
          <include>**/*catalog*.xml</include>
          <include>**/*.yaml</include>
        </includes>
      </resource>
      <resource>
        <directory>src/main/resources</directory>
      </resource>
    </resources>

    <plugins>

      <plugin>
        <groupId>edu.mayo.kmdp</groupId>
        <artifactId>terms-owl-to-skos-plugin</artifactId>
        <version>2.0.0</version>
        <configuration>
          <outputDirectory>${skosDir}</outputDirectory>
          <catalogURL>file:///${ontologyDir}${s}catalog.xml</catalogURL>
          <profile>SKOS</profile>
        </configuration>
        <executions>

          <execution>
            <id>lang-lang-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/KMDP/registry/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/KMDP/registry/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/BaseConstructedLanguage</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KRLanguage</skosNamespace>
              <skosOutputFile>KRLanguage.skos.rdf</skosOutputFile>
              <entityType>INST</entityType>
              <schemeName>KnowledgeRepresentationLanguages</schemeName>
              <topConceptName>KnowledgeRepresentationLanguage</topConceptName>
            </configuration>
          </execution>


          <execution>
            <id>lang-profile-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/KMDP/registry/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/KMDP/registry/</targetNamespace>
              <targetURI>http://www.omg.org/spec/DOL/DOL-terms/Profile</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KRProfile</skosNamespace>
              <skosOutputFile>KRProfile.skos.rdf</skosOutputFile>
              <entityType>INST</entityType>
              <schemeName>KnowledgeRepresentationLanguageProfiles</schemeName>
              <topConceptName>KnowledgeRepresentationLanguageProfile</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>lang-format-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/KMDP/registry/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/KMDP/registry/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/MetaFormat</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KRFormat</skosNamespace>
              <schemeName>SerializationFormats</schemeName>
              <topConceptName>SerializationFormat</topConceptName>
              <skosOutputFile>KRFormat.skos.rdf</skosOutputFile>
              <entityType>INST</entityType>
            </configuration>
          </execution>
          <execution>
            <id>lang-serial-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/KMDP/registry/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/KMDP/registry/</targetNamespace>
              <targetURI>http://www.omg.org/spec/DOL/DOL-terms/Serialization</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KRSerialization</skosNamespace>
              <skosOutputFile>KRSerialization.skos.rdf</skosOutputFile>
              <schemeName>KnowledgeRepresentationLanguageSerializations</schemeName>
              <topConceptName>KnowledgeRepresentationLanguageSerialization</topConceptName>
              <entityType>INST</entityType>
            </configuration>
          </execution>
          <execution>
            <id>lang-lex-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>http://ontology.mayo.edu/KMDP/registry/</owlFile>
              <targetNamespace>http://ontology.mayo.edu/KMDP/registry/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/Lexicon</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/Lexicon</skosNamespace>
              <skosOutputFile>Lexicon.skos.rdf</skosOutputFile>
              <schemeName>Lexica</schemeName>
              <topConceptName>Lexicon</topConceptName>
              <entityType>INST</entityType>
            </configuration>
          </execution>

          <execution>
            <id>lang-codes-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>${ontologyDir}/LanguageCodes.rdf</owlFile>
              <targetNamespace>http://www.omg.org/spec/LCC/Languages/ISO639-1-LanguageCodes/
              </targetNamespace>
              <targetURI>
                http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage
              </targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/ISO639-1-LanguageCodes</skosNamespace>
              <skosOutputFile>ISOLanguageCode.skos.rdf</skosOutputFile>
              <schemeName>KnowledgeRepresentationSerializations</schemeName>
              <topConceptName>KnowledgeRepresentationSerialization</topConceptName>
              <schemeName>Languages</schemeName>
              <topConceptName>Language</topConceptName>
              <entityType>INST</entityType>
            </configuration>
          </execution>

          <execution>
            <id>rel-deriv-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>https://www.omg.org/spec/API4KP/20190801/api4kp/</owlFile>
              <targetNamespace>https://www.omg.org/spec/API4KP/api4kp/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/derived-from</targetURI>
              <skosNamespace>https://ontology.mayo.edu/taxonomies/KAO/Rel/DerivationRelType</skosNamespace>
              <skosOutputFile>rel-deriv.skos.rdf</skosOutputFile>
              <schemeName>DerivationTypes</schemeName>
              <topConceptName>DerivationType</topConceptName>
            </configuration>
          </execution>
          <execution>
            <id>rel-variant-skos</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>owl-to-skos</goal>
            </goals>
            <configuration>
              <owlFile>https://www.omg.org/spec/API4KP/20190801/api4kp/</owlFile>
              <targetNamespace>https://www.omg.org/spec/API4KP/api4kp/</targetNamespace>
              <targetURI>https://www.omg.org/spec/API4KP/api4kp/variant-of</targetURI>
